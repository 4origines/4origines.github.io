{
  "name": "4origines.github.io",
  "version": "0.0.1",
  "description": "Website for OrigineBrass.com using 11ty.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf _site",
    "sass": "sass src/scss/app.scss dist/app.css",
    "build:sass": "sass --style=compressed src/scss/app.scss dist/app.css",
    "build:eleventy": "eleventy",
    "dev:sass": "npm run sass -- --watch",
    "dev:eleventy": "eleventy --serve --quiet",
    "dev": "npm-run-all clean -p dev:*",
    "build": "npm-run-all clean -s build:*"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4origines/4origines.github.io.git"
  },
  "keywords": [],
