  "license": "MIT",
  "browserslist": [
    "last 4 versions"
  ],
  "bugs": {
    "url": "https://github.com/4origines/4origines.github.io/issues"
  },
  "homepage": "https://github.com/4origines/4origines.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^2.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@orchidjs/eleventy-plugin-ids": "^0.1.1",
    "@popperjs/core": "^2.11.5",
    "autoprefixer": "^10.3.1",
    "cross-env": "^7.0.3",
    "del-cli": "^4.0.1",
    "fs-extra": "^8.1.0",
    "luxon": "^1.28.0",
    "npm-run-all": "^4.1.5",
